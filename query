select *
WHERE {
?animal dbo:kingdom dbr:Animal ;
 foaf:name ?nome ;
rdfs:label ?label;
dbo:abstract ?abstract;
rdfs:comment ?comment;
foaf:isPrimaryTopicOf ?link_wikipedia



#filter(lang(?nome) = "pt")
filter(lang(?label) = "pt" && lang(?abstract) = "pt" && lang(?comment) = "pt")

} limit 100




